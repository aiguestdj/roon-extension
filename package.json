{
    "name": "@aiguestdj/roon-extension",
    "version": "1.0.47",
    "description": "Roon extension to work with AI Guest DJ",
    "scripts": {
        "deploy": "npm run build && npm version patch --no-git-tag-version && npm publish",
        "docker": "PACKAGE_VERSION=$(grep '\"version\"' package.json | cut -d '\"' -f 4 | head -n 1) && docker buildx build --platform=linux/amd64,linux/arm64 -t aiguestdj/roon-extension -t aiguestdj/roon-extension:$PACKAGE_VERSION --push .",
        "dev": "next dev",
        "analyze": "ANALYZE=true next build",
        "build": "next build",
        "start": "next start",
        "lint": "next lint"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=10.0.0"
    },
    "bin": {
        "aiguestdj-roon": "./server.js"
    },
    "dependencies": {
        "@aiguestdj/shared": "^1.0.25",
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.15.3",
        "@mui/joy": "^5.0.0-beta.21",
        "@mui/lab": "^5.0.0-alpha.134",
        "axios": "^1.4.0",
        "next": "^14.2.0-canary.49",
        "next-compose-plugins": "^2.2.1",
        "next-connect": "^1.0.0",
        "notistack": "^3.0.1",
        "openai": "^4.28.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "roon-kit": "^0.1.0",
        "sass": "^1.63.3",
        "sharp": "^0.33.2"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^14.1.0",
        "@types/node": "^20.3.0",
        "@types/react": "^18.2.73",
        "@types/react-dom": "^18.2.23",
        "eslint": "8.42.0",
        "eslint-config-next": "^14.1.4",
        "terser-webpack-plugin": "^5.3.10"
    }
}
